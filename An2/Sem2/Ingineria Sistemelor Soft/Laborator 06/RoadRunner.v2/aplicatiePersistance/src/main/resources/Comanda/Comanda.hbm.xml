<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="aplicatie.model">
    <class name="aplicatie.model.Comanda" table="comanda">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="nume" column="nume"/>
        <property name="denumire" column="denumire"/>
        <property name="greutate" column="greutate"/>
        <property name="tipString" column="tip"/>
    </class>
</hibernate-mapping>
